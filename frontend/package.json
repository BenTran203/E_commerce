    "@tanstack/react-query": "^5.59.0",
    "axios": "^1.7.7",
    "clsx": "^2.1.1",
    "embla-carousel-react": "^8.3.0",
    "framer-motion": "^11.11.1",
    "i18next": "^25.5.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-locize-backend": "^7.0.4",
    "lucide-react": "^0.454.0",
    "next": "^16.0.7",
    "next-auth": "^5.0.0-beta.25",
    "react": "^19.2.1",
