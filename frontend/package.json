    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.14.0",
    "eslint-config-next": "^15.0.3",
    "locize-cli": "^10.3.2",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
